{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IdentityServer": {
    "Clients": {
      "InhouseCore.Web.Client": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "Jwt": {
    "Issuer": "https://localhost:5001",
    "Audience": "https://localhost:5001",
    "EncryptionKey": "{GenerateLongSecretStringHere}"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqliteConnectionString": "DataSource=\\Data\\{YourDataBaseName}.db",
    "DiscordBotToken": "{https://discord.com/developers/applications/{ApplicationId} BotToken}"
  },
  "Discord": {
    "ApplicationId": "{https://discord.com/developers/applications/ ApplicatonId without quotes}",
    "PlublicKey": "{https://discord.com/developers/applications/ PublicKey}",
    "ClientSecret": "{https://discord.com/developers/applications/{ApplicationId}/oauth2/general ClientSecret}",
    "AuthorizationEndpoint": "https://discord.com/oauth2/authorize",
    "TokenEndpoint": "https://discord.com/oauth2/token",
    "UserInformationEndpoint": "https://discord.com/api/usrs/@me"
  }
}
