    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IdentityServer": {
    "Clients": {
      "InhouseCore.Web.Client": {
        "Profile": "IdentityServerSPA"
      }
